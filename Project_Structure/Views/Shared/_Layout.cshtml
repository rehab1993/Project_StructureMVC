
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
</head>



<body>
    @*start header*@
    <nav class="mt-5">
        <center>
            <a class="btn btn-primary" asp-controller="Home" asp-action="Index">Home</a>
            <a class="btn btn-secondary" asp-controller="Home" asp-action="AboutUs">About Us</a>
            <a class="btn btn-success" asp-controller="Home" asp-action="Privacy">Privacy</a>
            <a class="btn btn-warning" asp-controller="Home" asp-action="ContactUs">Contact Us</a>
            <a class="btn btn-danger" asp-controller="Account" asp-action="Login">Login</a>
            <a class="btn btn-info" asp-controller="Account" asp-action="Register">Register</a>
        </center>
    </nav>
    @*end header*@
    <div>
        @RenderBody()
    </div>
    @*start Footer*@
    <center>
        <div>
            <p>CopyRights &copy; Route @DateTime.Now.Year </p>
        </div>
    </center>
    @*end footer*@
</body>
</html>
